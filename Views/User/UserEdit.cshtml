@model LeaveManagementSystem.ViewModel.EditUserViewModel;
<!DOCTYPE html>
    <html>
    <head>
        <title></title>
    </head>
    <body>
        <h1 class="text-primary text-center">User Update</h1>
        <form asp-action="EditUser" method="post">
        <input type="email" asp-for="Email" placeholder="Input user Email" class="form-control mt-4"/>
        <input type="text" asp-for="FirstName" placeholder="New User FirstName" class="form-control mt-4" />
        <input type="text" asp-for="LastName" placeholder="New User LastName" class="form-control mt-4" />
        <button class="btn btn-outline-primary mt-4">Update</button>
        </form>
    </body>
    </html>
